    "lefthook": "^1.5.2",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "tsc-watch": "6.0.4",
    "typescript": "5.2.2"
