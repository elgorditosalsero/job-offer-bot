    "@grammyjs/types": "3.3.0",
    "callback-data": "1.1.1",
    "dotenv": "16.3.1",
    "fastify": "4.24.3",
    "grammy": "1.19.2",
    "grammy-guard": "^0.5.0",
    "iso-639-1": "3.1.0",
    "mongodb": "^6.3.0",
    "node-graceful-shutdown": "1.1.5",
    "pino": "8.16.2",
    "pino-pretty": "10.2.3",
    "tsx": "4.6.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@types/node": "20.9.1",
    "@typescript-eslint/eslint-plugin": "6.11.0",
